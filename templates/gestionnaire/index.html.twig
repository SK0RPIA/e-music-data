{% extends 'base.html.twig' %}

{% block title %}
  Gestionnaire
{% endblock %}

{% block body %}

  <div class="col-lg-6">
                  <div class="card">
                    <div class="card-header">
                      <div class="card-close">
                        <div class="dropdown">
                          <button class="dropdown-toggle text-sm" type="button" id="closeCard1" data-bs-toggle="dropdown" aria-expanded="false"></button>
                          <div class="dropdown-menu dropdown-menu-end shadow-sm" aria-labelledby="closeCard1"><a class="dropdown-item py-1 px-3 remove" href="#"> <i class="fas fa-times"></i>Close</a><a class="dropdown-item py-1 px-3 edit" href="#"> <i class="fas fa-cog"></i>Edit</a></div>
                        </div>
                      </div>
                      <h3 class="h4 mb-0">Inscription Manuelle</h3>
                    </div>
                    <div class="card-body">
                      
                      <form>
                        <div class="mb-3">
                          <label class="form-label" for="exampleInputEmail1">Adresse Email</label>
                          <input class="form-control" id="exampleInputEmail1" type="email" aria-describedby="emailHelp">
                         
                        </div>
                        <div class="mb-3">
                          <label class="form-label" for="exampleInputPassword1">Mot de passe</label>
                          <input class="form-control" id="exampleInputPassword1" type="password">
                        </div>
                        <button class="btn btn-primary" type="submit">Inscrire</button>
                      </form>
                    </div>
                  </div>
                </div>
            
{% endblock %}

